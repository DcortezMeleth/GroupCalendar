<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
         version="3.1">
    <display-name>ejbs</display-name>
    <enterprise-beans>
        <session>
            <ejb-name>StartupBean</ejb-name>
            <env-entry>
                <env-entry-name>log4j/configFile</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>log4j2.xml</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>log4j/configPath</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>g:\jboss-as-7.1.1.Final\jboss-as-7.1.1.Final\standalone\configuration\</env-entry-value>
            </env-entry>
        </session>
    </enterprise-beans>
</ejb-jar>
